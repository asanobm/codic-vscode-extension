{
    "name": "codic",
    "displayName": "codic",
    "description": "[UNOFFICIAL] codic extension",
    "version": "0.0.1",
    "publisher": "pizzacat83",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.codictranslate"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "codic configuration",
            "properties": {
                "codic.ACCESS_TOKEN" : {
                    "type": "string",
                    "default": null, 
                    "description": "codic API Access Token"
                }
            }
        },
        "commands": [{
            "command": "extension.codictranslate",
            "title": "codic: Translate Japanese prase"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}